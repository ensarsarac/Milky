@model UpdateTeamDtos
@{
    ViewData["Title"] = "UpdateTeam";
    ViewData["InnerTitle"] = "Çalışanlar Alanı";
    Layout = "~/Areas/Admin/Views/Shared/_AdminLayout.cshtml";
}
<div class="row">
    <div class="col-md-12">
        <div class="white_shd full margin_bottom_30">
            <div class="full graph_head">
                <div class="heading1 margin_0">
                    <h2>Çalışan Güncelle</h2>
                </div>
            </div>
            <div class="table_section padding_infor_info">
                <form method="post">
                    <input type="hidden" asp-for="TeamId" />
                    <div class="form-group">
                        <label class="form-label">Ad Soyad</label>
                        <input type="text" class="form-control" asp-for="NameSurname" placeholder="Ad Soyad" />
                    </div>
                    <div class="form-group">
                        <label class="form-label">Görsel Linki</label>
                        <input type="text" class="form-control" asp-for="ImageUrl" placeholder="Görsel Linki" />
                    </div>
                    <div class="form-group">
                        <label class="form-label">Ünvan</label>
                        <input type="text" class="form-control" asp-for="Title" placeholder="Ünvan" />
                    </div>
                    <button type="submit" class="btn btn-outline-warning">Güncelle</button>
                </form>
            </div>
        </div>
    </div>
</div>